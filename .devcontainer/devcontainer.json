{
  "name": "DevBoxContainer",
  "image": "mcr.microsoft.com/devcontainers/go",
  "features": {
    "ghcr.io/dlouwers/devcontainer-features/devbox:1" : {},
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}	
  },
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": [
        "jetpack-io.devbox",
        "golang.go",
        "hashicorp.terraform"
      ]
    }
  },
  "postCreateCommand": "devbox install",
  "forwardPorts": [
    8080
  ]
}

